<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net6.0;net7.0;net8.0;net9.0</TargetFrameworks>
    <AssemblyVersion>1.0.2.0</AssemblyVersion>
    <FileVersion>1.0.2.0</FileVersion>
    <Company>Orbital7</Company>
    <Copyright>Copyright Â© 2025 Orbital7</Copyright>
    <Version>1.0.2</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>.NET *Unofficial* library for ScrapingBee API (https://scrapingbee.com)</Description>
    <Authors>Josh van Eikeren</Authors>
    <PackageLicenseUrl></PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/orbital7/orbital7.apis.scrapingbee</PackageProjectUrl>
    <PackageIconUrl></PackageIconUrl>
    <LangVersion>latest</LangVersion>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIcon>Orbital7.png</PackageIcon>
	<PackageReadmeFile>Readme.md</PackageReadmeFile>
	<AccelerateBuildsInVisualStudio>true</AccelerateBuildsInVisualStudio>
  </PropertyGroup>
	
  <ItemGroup>
	<None Include="../../Readme.md">
      <Pack>True</Pack>
	  <PackagePath></PackagePath>
	</None>
  </ItemGroup>
	
  <ItemGroup>
	<None Include="../../Orbital7.png">
      <Pack>True</Pack>
	  <PackagePath></PackagePath>
	</None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Text.Json" Version="9.0.3" Condition="'$(TargetFramework)' == 'netstandard2.0'" />
  </ItemGroup>
	
</Project>
